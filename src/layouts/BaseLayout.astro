---
import Navbar from "../components/Navbar.astro";
import BaseHead from "../components/BaseHead.astro";

const { ...head } = Astro.props;
---

<html lang="en">
  <head>
    <BaseHead {...head} />
  </head>
  <body class="w-full" transition:animate="slide">
    <Navbar class="bg-slate-50 border-b border-slate-100" />
    <slot />
  </body>
</html>
